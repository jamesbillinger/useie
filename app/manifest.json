{
  "manifest_version": 2,
  "minimum_chrome_version": "28",
  "offline_enabled": true,
  "name": "Use IE",
  "description": "Forces certain URL's to launch in IE instead",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "permissions": [
    "nativeMessaging",
    "storage",
    "webNavigation"
  ]
}